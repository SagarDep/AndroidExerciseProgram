<?xml version="1.0" encoding="utf-8"?>
<com.oubowu.exerciseprogram.toucheventdelivery.TouchEventFrameLayout
    android:id="@+id/governor"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/material_yellow_50"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.oubowu.exerciseprogram.toucheventdelivery.TouchEventActivity">

    <com.oubowu.exerciseprogram.toucheventdelivery.TouchEventRelativeLayout
        android:id="@+id/mayor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="40dp"
        android:background="@color/material_green_100">

        <com.oubowu.exerciseprogram.toucheventdelivery.TouchEventTextView
            android:id="@+id/farmer"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerInParent="true"
            android:background="@color/material_red_100"
            android:gravity="center"
            android:text="农民"
            android:textSize="30sp"/>

    </com.oubowu.exerciseprogram.toucheventdelivery.TouchEventRelativeLayout>

</com.oubowu.exerciseprogram.toucheventdelivery.TouchEventFrameLayout>
